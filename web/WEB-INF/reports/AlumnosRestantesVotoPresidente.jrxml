	<queryString>
		<![CDATA[SELECT 

	a.nombre AS 'Nombre',

	e.nombre AS 'Especialidad', 

	a.codigo AS 'Codigo', 

	v.imagen_src AS 'Imagen',

	a.NIE AS 'NIE',
	(
	SELECT 
		COUNT(*) AS 'Total Votantes' 
	FROM 
		Votantes AS v
	) AS 'Total Votantes',
	( 
	SELECT 
		COUNT(*) 
	FROM 
		Votantes v
	WHERE 
		v.voto_realizado = FALSE	
	) AS 'No han votado'
FROM 

	Votantes AS v 

INNER JOIN
	Alumno AS a
	ON a.codigo = v.alumno
INNER JOIN 

	Especialidad AS e 
	ON e.codigo = a.especialidad

WHERE 
	voto_realizado = FALSE
ORDER BY
	v.especialidad;]]>
	</queryString>
