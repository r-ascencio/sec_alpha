	<queryString>
		<![CDATA[SELECT 


	a.nombre AS 'Nombre',

	e.nombre AS 'Especialidad', 

	a.codigo AS 'Codigo', 

	a.NIE AS 'NIE',

	(
	
	SELECT 
		COUNT(*) AS 'Total Alumnos' 
	FROM 
		Alumno AS a
	WHERE 
		a.especialidad = $P{Especialidad}
	) AS 'Total de Alumnos',
	
	( 
	SELECT 
		COUNT(*) 
	FROM 
		Alumno a
	WHERE 
		a.especialidad = $P{Especialidad}
		AND a.voto = FALSE	
	) AS 'Alumnos sin votar'
	
FROM 

	Alumno AS a 

INNER JOIN 

	Especialidad AS e 
	ON e.codigo = a.especialidad

WHERE 
	voto = FALSE
	AND a.especialidad = $P{Especialidad};]]>
	</queryString>
