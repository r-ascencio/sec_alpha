	<queryString>
		<![CDATA[SELECT 

	a.nombre AS 'Nombre',

	e.nombre AS 'Especialidad', 

	a.codigo AS 'Codigo', 

	a.NIE AS 'NIE',

	p.puntaje AS 'Puntaje',

	p.imagen_src AS 'Imagen'

FROM 

	Presidentes AS p 

INNER JOIN
	Alumno AS a
	ON a.codigo = p.alumno
INNER JOIN 

	Especialidad AS e 
	ON e.codigo = a.especialidad
ORDER BY
	p.puntaje DESC;]]>
